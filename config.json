{
	"Port": "80",
	"Hostname": "hostbaitor.com",
	"TemplateDir": "/home/wiki/templates/*.html",
	"Redirects": [
		{
			"Requested": "/favicon.ico",
			"Target": "/site/favicon.ico",
			"Code": 301
		},
		{
			"Requested": "/rawindex",
			"Target": "/raw/index.md",
			"Code": 307
		},
		{
			"Requested": "/tag/none/",
			"Target": "/",
			"Code": 307
		},
		{
			"Requested": "/author/none/",
			"Target": "/",
			"Code": 307
		}
	],
	"Indexes": [
		{
			"WatchDirs": {
				"/home/wiki/pages/": "/"
			},
			"WatchExtension": ".md",
			"IndexPath": "/home/wiki/index/",
			"IndexType": "en",
			"IndexName": "wiki",
			"Restricted": [],
			"Handlers": [
				{
					"Path": "/home/wiki/pages",
					"Prefix": "/",
					"Default": "readme",
					"ServerType": "markdown",
					"Template": "wiki.html",
					"Restricted": []
				},
				{
					"Path": "/home/wiki/static",
					"Prefix": "/static/",
					"Default": "",
					"ServerType": "raw",
					"Restricted": [
						"ini",
						"json"
					]
				},
				{
					"Path": "/home/wiki/index",
					"Prefix": "/search/",
					"Default": "wiki",
					"Template": "search.html",
					"ServerType": "fuzzy",
					"FallbackTemplate":"search.html"
				},
				{
					"ServerType": "fieldList",
					"Prefix": "/topic/",
					"Path": "/home/wiki/index",
					"Default": "topic",
					"Template": "topicList.html",
					"FallbackTemplate":"topicList.html"
				},
				{
					"ServerType": "fieldList",
					"Prefix": "/author/",
					"Path": "/home/wiki/index",
					"Default": "author",
					"Template": "authorList.html",
					"FallbackTemplate":"authorList.html"
				}
			]
		}
	]
}