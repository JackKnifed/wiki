{
  "Global": {
    "Port": "80",
    "Hostname": "hostbaitor.com",
    "TemplateDir": "/home/wiki/backend/templates"
  },
  "Redirects": [
    {
      "Requested": "/favicon.ico",
      "Target": "/site/favicon.ico",
      "Code": 301
    },
    {
      "Requested": "/rawindex",
      "Target": "/raw/index.md",
      "Code": 307
    },
    {
      "Requested": "/tag/none/",
      "Target": "/",
      "Code": 307
    },
    {
      "Requested": "/author/none/",
      "Target": "/",
      "Code": 307
    }
  ],
  "Indexes": [
    {
      "WatchDirs": {
        "/home/wiki/frontend/": "/"
      },
      "WatchExtension": ".md",
      "IndexPath": "/home/wiki/index/",
      "IndexType": "en",
      "IndexName": "wiki",
      "Restricted": [
        "internal",
        "handbook"
      ]
    }
  ],
  "Server": [
    {
      "Path": "/home/wiki/frontend/",
      "Prefix": "/",
      "Default": "readme",
      "ServerType": "markdown",
      "Template": "wiki.html",
      "Restricted": [
        "internal",
        "handbook"
      ]
    },
    {
      "Path": "/home/wiki/index/",
      "Prefix": "/search/",
      "Default": "wiki",
      "Template": "search.html",
      "ServerType": "search"
    },
    {
      "Path": "/home/wiki/frontend/",
      "Prefix": "/raw/",
			"Default": "index",
      "ServerType": "raw",
      "Restricted": [
        "md",
        "ini",
        "json"
      ]
    },
    {
      "Path": "/home/wiki/backend/site/",
      "Prefix": "/site/",
			"Default": "",
      "ServerType": "raw",
      "Restricted": [
        "ini",
        "json"
      ]
    },
    {
      "ServerType": "fieldList",
      "Prefix": "/tag/",
      "Path": "/home/wiki/index",
      "Default": "tag",
      "Template": "topicList.html"
    },
    {
      "ServerType": "fieldList",
      "Prefix": "/author/",
      "Path": "/home/wiki/index",
      "Default": "author",
      "Template": "authorList.html"
    }
  ]
}
